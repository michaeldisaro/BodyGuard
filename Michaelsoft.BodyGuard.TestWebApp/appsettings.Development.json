{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "BodyGuardClientSettings": {
    "ServerBasePath": "https://localhost:5001",
    "ApplicationBasePath": "https://localhost:5002"
  },
  "CommonSettings": {
    "BaseApplicationPath" : "https://localhost:5002",
    "IdentitySettings": {
      "AdditionalClaims": [
        "emailaddress",
        "fullname"
      ],
      "CustomRoles": [
        "director"
      ],
      "EnabledUserDataProperties": [
        "EmailAddress",
        "Name",
        "Surname"
      ]
    },
    "PasswordSettings": {
      "PasswordShouldContainAtLeastOneSymbol": false
    }
  }
}
